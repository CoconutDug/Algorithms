{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++.exe compilar archivo activo",
      "command": "C:\\msys64\\mingw64\\bin\\g++.exe",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/sort/sorting_functions.o",
        "${workspaceFolder}/utilities/files_utilities/file_utilities.o",
        "${workspaceFolder}/utilities/vector_utilities/utilities_vectors.o",
        "${workspaceFolder}/search/searching_functions.o",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Tarea generada por el depurador."
    },
    {
      "type": "cppbuild",
      "label": "Run the compiled cpp",
      "command": "${fileDirname}\\${fileBasenameNoExtension}.exe"
    }
  ],
  "version": "2.0.0"
